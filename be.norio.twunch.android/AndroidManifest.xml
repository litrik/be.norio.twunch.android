<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="be.norio.twunch.android"
	android:versionCode="5" android:versionName="0.5">
	<application android:icon="@drawable/icon" android:label="@string/app_name" android:name="TwunchApplication"
		android:theme="@android:style/Theme.Light">
		<activity android:name=".TwunchesActivity" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".TwunchActivity" android:label="@string/activity_twunch" />
		<activity android:name=".AboutActivity" android:label="@string/activity_about" />

	</application>

	<uses-sdk android:minSdkVersion="4"></uses-sdk>

	<supports-screens android:smallScreens="true" android:largeScreens="true" android:anyDensity="true" />

	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>

</manifest> 