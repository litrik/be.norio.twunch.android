<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="be.norio.twunch.android"
	android:versionCode="10" android:versionName="1.1.0">

	<application android:icon="@drawable/icon" android:label="@string/app_name" android:name="TwunchApplication"
		android:theme="@style/Theme.Twunch">

		<uses-library android:name="com.google.android.maps" />

		<activity android:name=".TwunchesActivity" android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".TwunchesMapActivity" android:label="@string/activity_twunches" />
		<activity android:name=".TwunchActivity" android:label="@string/activity_twunch" />
		<activity android:name=".AboutActivity" android:label="@string/activity_about" />
	</application>

	<uses-sdk android:minSdkVersion="7"></uses-sdk>

	<supports-screens android:smallScreens="true" android:largeScreens="true" android:anyDensity="true" />

	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
	<uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>

</manifest> 